{
    "name": "sails-idl-syntax",
    "displayName": "Sails IDL Syntax Highlighter",
    "description": "Syntax highlighting for Gear Sails IDL files",
    "version": "0.1.0",
    "engines": {
      "vscode": "^1.80.0"
    },
    "categories": ["Programming Languages"],
    "contributes": {
      "languages": [
        {
          "id": "sails-idl",
          "aliases": ["Sails IDL"],
          "extensions": [".idl"],
          "configuration": "./language-configuration.json"
        }
      ],
      "grammars": [
        {
          "language": "sails-idl",
          "scopeName": "source.sails-idl",
          "path": "./syntaxes/sails-idl.tmLanguage.json"
        }
      ]
    },
    "repository": "https://github.com/nikvolf/sails-vscode"
  }
